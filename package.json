{
  "name": "discord_music_bot",
  "version": "1.0.0",
  "description": "Music bot for discord (because Groovy support is stopped...)",
  "main": "dist/server.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/server.js\"",
    "prestart": "npm run build",
    "heroku-postbuild": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "npx ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Martin Lehtomaa",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "@types/node": "^16.7.2",
    "@types/spotify-web-api-node": "^5.0.7",
    "cross-env": "^7.0.3",
    "discord.js": "^13.6.0",
    "dotenv": "^10.0.0",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^4.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^4.6.0",
    "inversify": "^5.1.1",
    "libsodium-wrappers": "^0.7.9",
    "nodemon": "^2.0.12",
    "play-dl": "^1.9.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "spotify-web-api-node": "^5.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "youtube-dl-exec": "^2.0.5",
    "ytdl-core": "^4.11.2",
    "ytdl-core-discord": "^1.3.1",
    "ytsr": "^3.5.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.1"
  }
}
